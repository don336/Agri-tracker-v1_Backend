{
  "name": "agri-Tracker",
  "version": "1.0.0",
  "scripts": {
    "build": "babel ./src --out-dir ./build",
    "start": "node build/index.js",
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "prettier:check": "prettier --check .",
    "prettier": "prettier --write .",
    "dev": "env-cmd -f ./config/dev.env nodemon --watch ./src --exec ts-node src/index.ts",
    "test": "env-cmd -f ./config/test.env jest --runInBand --verbose --coverage --force-exit src/tests/auth.test.js src/tests/crop.test.js src/tests/customer.test.js src/tests/sales.test.js src/tests/stock.test.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "celebrate": "^15.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.21.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.5",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "uuid": "^11.0.5",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.19.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "body-parser": "^1.20.3",
    "compression": "^1.8.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.21.0"
  }
}
